# Address where the app will run
LEPTOS_SITE_ADDR=127.0.0.1:3000

# Live reload port. Only needed for development purposes.
LEPTOS_RELOAD_PORT=3001

# OAuth2 configuraiton
OAUTH2_CLIENT_ID=
